{
  "type": "wordpress-plugin",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "automattic/jetpack-autoloader": "^1.2.0",
    "woocommerce/woocommerce-sniffs": "^0.0.9"
  },
  "require-dev": {
    "brain/monkey": "^2.3",
    "phpunit/phpunit": "^8.4"
  },
  "autoload": {
    "psr-4": {
      "WCVendors\\": "src/"
    },
    "files": [
      "./inc/wcv-core-functions.php",
      "./inc/wcv-vendor-functions.php",
      "./inc/wcv-admin-functions.php"
    ]
  }
}
